{
  "name": "planning-poker",
  "version": "1.0.0",
  "description": "Create tasks, estimate them with your coworkers and visualize results in real-time",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:debug": "nodemon",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"app/**/*.ts\"",
    "lint": "eslint 'app/**/*.ts' --quiet --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shinomix/planning-poker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shinomix/planning-poker/issues"
  },
  "homepage": "https://github.com/Shinomix/planning-poker#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "polka": "^0.5.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "rootDir": "app",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/app/specs/**/*spec.+(ts)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
